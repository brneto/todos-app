{
  "name": "todos-app",
  "version": "0.0.1",
  "private": true,
  "description": "Add todos",
  "main": "src/server/index.js",
  "scripts": {
    "pull": "git pull && yarn && yarn outdated",
    "prestart": "yarn lint",
    "start": "pm2 start ecosystem.config.js",
    "poststart": "run-p watch:*",
    "stop": "pm2 stop ecosystem.config.js",
    "poststop": "pm2 flush; pm2 delete ecosystem.config.js; yarn rmcache; exit 0",
    "ps": "pm2 l",
    "lint:js": "esw src",
    "lint:css": "stylelint 'src/client/**/*.js'",
    "lint": "run-s lint:*",
    "watch:lint": "yarn lint:js --watch",
    "test": "jest",
    "watch:test": "TERM=dumb yarn test --watch",
    "prebuild": "yarn lint test poststop",
    "build": "pm2 start ecosystem.config.js --env production",
    "postbuild": "yarn watch:app",
    "watch:app": "tail -n +1 -f logs/app-err.log logs/app-out.log",
    "rmcache": "rm -rf node_modules/.cache/hard-source/*"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@material)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com:learning-concepts/redux.git"
  },
  "keywords": [
    "javascript",
    "react",
    "react-router",
    "redux"
  ],
  "author": "Berchris Requiao",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/learning-concepts/redux/issues"
  },
  "homepage": "https://gitlab.com/learning-concepts/redux#README",
