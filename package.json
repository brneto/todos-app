{
  "name": "todos-app",
  "version": "0.0.1",
  "private": true,
  "description": "Add todos",
  "main": "dist/index.js",
  "scripts": {
    "reach": "git pull && yarn && yarn outdated",
    "upgi": "yarn upgrade-interactive --latest",
    "lint": "run-s lint:*",
    "lint:js": "eslint src",
    "lint:css": "stylelint src/client/**/*.js",
    "test": "jest",
    "watch:test": "TERM=dumb yarn test --watch",
    "prestart": "yarn stop && yarn lint && yarn test",
    "start": "pm2 start ecosystem.config.js",
    "poststart": "yarn logs",
    "ps": "pm2 l",
    "logs": "pm2 logs",
    "stop": "pm2 stop ecosystem.config.js",
    "poststop": "pm2 flush && pm2 del ecosystem.config.js && yarn rmcache",
    "rmcache": "rm -rf node_modules/.cache/hard-source dist",
    "prebuild": "yarn prestart",
    "build": "run-s build:*",
    "build:client": "NODE_ENV=building babel-node src/index.js",
    "build:server": "babel src -d dist --minified --only 'src/*.js','src/server/**/*.js' --ignore 'src/**/*.test.*','src/**/*.spec.*'"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/tests.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@material)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:brneto/todos-app.git"
  },
  "keywords": [
    "javascript",
    "react",
    "react-router",
    "redux"
  ],
  "author": "Berchris Requiao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brneto/todos-app/issues"
  },
  "homepage": "https://github.com/brneto/todos-app",
  "dependencies": {
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-function-bind": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-proposal-pipeline-operator": "^7.7.7",
    "@babel/plugin-proposal-private-methods": "^7.7.4",
    "@babel/plugin-proposal-throw-expressions": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@hot-loader/react-dom": "^16.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-styled-components": "^1.10.6",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
